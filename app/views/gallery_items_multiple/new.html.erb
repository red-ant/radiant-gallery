<div class="form-area">
	<div class="form-container">
		<div class="form" style="text-align:center">
			<% session_key = ActionController::Base.session_options[:session_key] %>
			<% @item = @gallery.items.new %>
			<% form_for @item, :url => admin_gallery_upload_multiple_path(session_key => cookies[session_key], request_forgery_protection_token => form_authenticity_token), :html => {:id => 'multiple_uploads'} do |f| %>
				<span id="spanBtnPlaceHolder"></span>
			<% end %>
			<div id="upload_queue" style="margin:20px 0"></div>
			<a href="javascript: self.close();">CLOSE</a>
		</div>
	</div>
</div>